<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  
  <display-name>Archetype Created Web Application</display-name>
  
   <servlet>
    <servlet-name>authorizationServlet</servlet-name>
    <servlet-class>carShop.AuthorizationServlet</servlet-class>
  </servlet>
  
   <servlet>
    <servlet-name>printInformation</servlet-name>
    <servlet-class>carShop.PrintInformationServlet</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  
   <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>carShop.LogoutServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>authorizationServlet</servlet-name>
    <url-pattern>/myAccount</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>printInformation</servlet-name>
    <url-pattern>/printInformation</url-pattern>
  </servlet-mapping>
  
  
  <welcome-file-list>  
    <welcome-file>index.html</welcome-file>  
  </welcome-file-list> 

</web-app>
